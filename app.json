{
  "expo": {
    "name": "gatgu",
    "slug": "gatgu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wafflestudio.gatgu"
    },
    "android": {
      "package": "com.wafflestudio.gatgu",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo/Logo13.svg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/Logo/Logo13.svg"
    }
  }
}
